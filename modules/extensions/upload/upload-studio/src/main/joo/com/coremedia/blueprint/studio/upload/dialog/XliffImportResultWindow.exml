<?xml version="1.0" encoding="ISO-8859-1"?>
<exml:component xmlns:exml="http://www.jangaroo.net/exml/0.8"
                xmlns="exml:ext.config"
                xmlns:editor="exml:com.coremedia.cms.editor.sdk.config">

  <exml:import class="com.coremedia.blueprint.studio.UploadStudioPlugin_properties"/>
  <exml:import class="com.coremedia.blueprint.studio.upload.XliffImportResultCodes"/>

  <exml:cfg name="successful" type="Boolean"/>

  <editor:bulkResultWindow
          title="{config.successful ?
                  UploadStudioPlugin_properties.INSTANCE.bulkResult_success_window_title :
                  UploadStudioPlugin_properties.INSTANCE.bulkResult_error_window_title}"
          bulkResultItems="{config.bulkResultItems}"
          resourceBundle="{UploadStudioPlugin_properties.INSTANCE}"
          errorCodes="{[
                XliffImportResultCodes.FAILED,
                XliffImportResultCodes.DOES_NOT_EXIST,
                XliffImportResultCodes.MASTER_CHANGED,
                XliffImportResultCodes.MASTER_VERSION_OUTDATED,
                XliffImportResultCodes.INVALID_XLIFF,
                XliffImportResultCodes.INVALID_CONTENT_ID,
                XliffImportResultCodes.NO_SUCH_PROPERTY,
                XliffImportResultCodes.ALREADY_CHECKED_OUT,
                XliffImportResultCodes.STRING_TOO_LONG,
                XliffImportResultCodes.STRING_LIST_TOO_LONG,
                XliffImportResultCodes.INVALID_MARKUP,
                XliffImportResultCodes.DUPLICATE_NAME,
                XliffImportResultCodes.INVALID_INTERNAL_LINK,
                XliffImportResultCodes.INVALID_LOCALE,
                XliffImportResultCodes.EMPTY_TRANSUNIT_TARGET,
                XliffImportResultCodes.EMPTY_TRANSUNIT_TARGET_FOR_WHITESPACE_SOURCE
              ]}"/>

</exml:component>
