{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "es5",
      "es6",
      "esnext"
    ],
    "module": "commonJS",
    "moduleResolution": "Node",
    "newLine": "LF",
    "removeComments": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
    "composite": true,
    "incremental": true,
    "types": [
      "@ckeditor/ckeditor5-editor-classic"
    ],
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "../dist",
    "declaration": true,
    "declarationMap": true,
    "paths": {
      "@coremedia/studio-client.ckeditor-constants/*": [
        "../node_modules/@coremedia/studio-client.ckeditor-constants/src/*"
      ],
      "@coremedia/studio-client.cap-base-models/*": [
        "../node_modules/@coremedia/studio-client.cap-base-models/src/*"
      ],
      "@coremedia/ckeditor5-studio-essentials/*": [
        "../node_modules/@coremedia/ckeditor5-studio-essentials/dist/*"
      ],
      "@coremedia/ckeditor5-coremedia-link/*": [
        "../node_modules/@coremedia/ckeditor5-coremedia-link/dist/*"
      ],
      "@coremedia/ckeditor5-coremedia-content-clipboard/*": [
        "../node_modules/@coremedia/ckeditor5-coremedia-content-clipboard/dist/*"
      ],
      "@coremedia/ckeditor5-coremedia-images/*": [
        "../node_modules/@coremedia/ckeditor5-coremedia-images/dist/*"
      ],
      "@coremedia/ckeditor5-symbol-on-paste-mapper/*": [
        "../node_modules/@coremedia/ckeditor5-symbol-on-paste-mapper/dist/*"
      ],
      "@jangaroo/runtime/*": [
        "../node_modules/@jangaroo/runtime/src/*"
      ]
    }
  }
}
