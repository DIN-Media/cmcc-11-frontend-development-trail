<?xml version="1.0" encoding="UTF-8"?>
<editor:DashboardConfiguration
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:exml="http://www.jangaroo.net/exml/0.8"
        xmlns:u="exml:untyped"
        xmlns="exml:ext.config"
        xmlns:editor="exml:com.coremedia.cms.editor.sdk.config">
  <fx:Script><![CDATA[
    public static const xtype:String = "acme.config.customWidgetTitleDashboardConfiguration";

    private var config:CustomWidgetTitleDashboardConfiguration;

    public native function CustomWidgetTitleDashboardConfiguration(config:CustomWidgetTitleDashboardConfiguration = null);
    ]]></fx:Script>

  <editor:widgets>
    <editor:WidgetState widgetTypeId="aWidgetType"
                        u:title="customTitle"/>
  </editor:widgets>

  <editor:types>
    <editor:ComponentBasedWidgetType id_="aWidgetType"
                                     name="aWidget"
                                     description="This is a widget.">
      <editor:widgetComponent>
        <Container>
          <items>
            <Label text="Widget"/>
          </items>
        </Container>
      </editor:widgetComponent>
    </editor:ComponentBasedWidgetType>
  </editor:types>

</editor:DashboardConfiguration>
