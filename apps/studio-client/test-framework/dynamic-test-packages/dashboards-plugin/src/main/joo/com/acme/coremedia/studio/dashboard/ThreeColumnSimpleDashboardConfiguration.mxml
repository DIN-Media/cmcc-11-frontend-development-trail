    public static const xtype:String = "acme.config.threeColumnSimpleDashboardConfiguration";

    private var config:ThreeColumnSimpleDashboardConfiguration;

    public native function ThreeColumnSimpleDashboardConfiguration(config:ThreeColumnSimpleDashboardConfiguration = null);
    ]]></fx:Script>

  <editor:widgets>
