            <Label text="Widget"/>
          </items>
        </Container>
      </editor:widgetComponent>
    </editor:ComponentBasedWidgetType>
    <editor:ComponentBasedWidgetType id_="anotherWidgetType"
                                     name="anotherWidget"
                                     description="This is another widget.">
      <editor:widgetComponent>
        <Container>
          <items>
            <Label text="Another Widget"/>
          </items>
        </Container>
      </editor:widgetComponent>
