<?xml version="1.0" encoding="UTF-8"?>
<local:AnalyticsProviderBase
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:local="com.coremedia.blueprint.studio.analytics.*"
        xmlns:exml="http://www.jangaroo.net/exml/0.8"
        xmlns="exml:ext.config">
  <fx:Script><![CDATA[
    private var config:AnalyticsProvider;

    public native function AnalyticsProvider(config:AnalyticsProvider = null);

    /**
     * The provider name of the analytics service provider
     */
    [ExtConfig]
    public var providerName:String;

    /**
     * The localized provider name of the analytics service provider
     */
    [ExtConfig]
    public var localizedProviderName:String;
    ]]></fx:Script>

</local:AnalyticsProviderBase>
