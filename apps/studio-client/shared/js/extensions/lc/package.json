{
  "name": "@coremedia-blueprint/studio-client.lc",
  "version": "1.0.0-SNAPSHOT",
  "author": "CoreMedia GmbH",
  "license": "CoreMedia Open Source License",
  "dependencies": {
    "@coremedia/studio-client.cap-base-models": "2204.1.0",
    "@jangaroo/runtime": "^1.1.3"
  },
  "devDependencies": {
    "@jangaroo/build": "^1.1.3",
    "@jangaroo/core": "^1.1.3",
    "@jangaroo/eslint-config": "^1.1.3",
    "@jangaroo/publish": "^1.1.3",
    "eslint": "^7.27.0",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./build",
    "build": "jangaroo build",
    "watch": "jangaroo watch",
    "publish": "jangaroo publish",
    "lint": "eslint --fix \"src/**/*.ts\""
  },
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/src/index.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./dist/src/*.js"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "types": "./src/index.d.ts",
    "exports": {
      ".": {
        "types": "./src/index.d.ts",
        "default": "./src/index.js"
      },
      "./*": {
        "types": "./src/*.d.ts",
        "default": "./src/*.js"
      }
    }
  }
}
